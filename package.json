{
    "name": "rn-template",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "android": "react-native run-android",
        "ios": "react-native run-ios",
        "start": "react-native start --reset-cache",
        "eslint": "eslint --fix --ext .ts,.tsx src/",
        "prettier": "prettier --check src/**/*.tsx --write",
        "postinstall": "husky install",
        "clean:android": "cd android && ./gradlew cleanBuildCache && ./gradlew clean && cd ..",
        "build:android": "cd android && ./gradlew assembleRelease && cd ..",
        "clean:ios": "rm -rf ios/build",
        "clean:node": "rm -rf node_modules",
        "gen-icon": "npx iconfont-rn",
        "clean": "run-p clean:*",
        "commit": "git-cz",
        "xcode": "xed xcode",
        "pod": "cd ios && pod install --repo-update && cd ..",
        "pod-install": "cd ios && pod install && cd ..",
        "test": "jest --passWithNoTests --detectOpenHandles --watch",
        "e2e:ios:test": "detox test -c ios",
        "e2e:ios:build": "detox build -c ios",
        "e2e:android:test": "detox test -c android",
        "e2e:android:build": "detox build -c android",
        "ycore": "yalc add @next-dev/core",
        "yhook": "yalc add @next-dev/hooks",
        "ycore-update": "yalc add @next-dev/core",
        "yhook-update": "yalc add @next-dev/hooks",
        "y-add": "yarn ycore && yarn yhook",
        "y-update": "yarn ycore-update && yarn yhook-update"
    },
    "config": {
        "commitizen": {
            "path": "node_modules/cz-customizable"
        }
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run tsc && npm run eslint:fix && npm run lint:fix && lint-staged",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "lint-staged": {
        "src/**/*.{ts,tsx}": [
            "git add ."
        ]
    },
    "dependencies": {
        "@ant-design/icons-react-native": "^2.2.1",
        "@ant-design/react-native": "^4.0.7",
        "@next-dev/core": "file:.yalc/@next-dev/core",
        "@next-dev/hooks": "file:.yalc/@next-dev/hooks",
        "@react-native-community/async-storage": "^1.12.1",
        "@react-native-community/cameraroll": "^4.0.4",
        "@react-native-community/masked-view": "^0.1.10",
        "@react-native-community/picker": "^1.8.1",
        "@react-native-community/segmented-control": "^2.2.2",
        "@react-native-community/slider": "^3.0.3",
        "@react-native-community/viewpager": "^5.0.11",
        "@react-native-picker/picker": "^1.14.0",
        "@react-navigation/bottom-tabs": "^5.11.8",
        "@react-navigation/native": "^5.9.3",
        "@react-navigation/stack": "^5.14.3",
        "add": "^2.0.6",
        "ahooks": "^2.10.0",
        "immer": "^9.0.1",
        "jotai": "^0.15.3",
        "lodash-es": "^4.17.21",
        "multiselect-react-dropdown": "^1.6.11",
        "rc-field-form": "^1.20.0",
        "react": "^17.0.2",
        "react-error-boundary": "^3.1.1",
        "react-native": "0.64.0",
        "react-native-bootsplash": "^3.2.0",
        "react-native-currency-input": "^0.1.3",
        "react-native-dotenv": "^2.5.3",
        "react-native-exception-handler": "^2.10.9",
        "react-native-gesture-handler": "^1.10.3",
        "react-native-keyboard-aware-scroll-view": "^0.9.3",
        "react-native-mmkv": "^1.1.6",
        "react-native-multi-selectbox": "^1.4.9",
        "react-native-multiple-select": "^0.5.6",
        "react-native-multiple-select-list": "^1.0.4",
        "react-native-picklist": "^1.5.0",
        "react-native-reanimated": "1.13.3",
        "react-native-redash": "14.2.4",
        "react-native-restart": "^0.0.22",
        "react-native-safe-area-context": "^3.2.0",
        "react-native-screens": "^2.18.1",
        "react-native-svg": "^12.1.0",
        "react-native-vector-icons": "^8.1.0",
        "react-native-webp-format": "^1.1.2",
        "swr": "^0.5.4",
        "umi-request": "^1.3.5",
        "yarn": "^1.22.10"
    },
    "devDependencies": {
        "@babel/core": "^7.12.9",
        "@babel/runtime": "^7.12.5",
        "@bam.tech/react-native-make": "^3.0.0",
        "@commitlint/cli": "^12.0.1",
        "@commitlint/config-conventional": "^12.0.1",
        "@react-native-community/eslint-config": "^2.0.0",
        "@testing-library/jest-native": "^4.0.1",
        "@testing-library/react-hooks": "^5.0.3",
        "@testing-library/react-native": "^7.1.0",
        "@types/detox": "^17.14.0",
        "@types/jest": "^26.0.20",
        "@types/react-native": "^0.64.0",
        "@types/react-native-vector-icons": "^6.4.6",
        "@types/react-test-renderer": "^17.0.1",
        "@typescript-eslint/eslint-plugin": "^4.15.1",
        "@typescript-eslint/parser": "^4.15.1",
        "antd-mobile-demo-data": "^0.3.0",
        "babel-jest": "^26.6.3",
        "babel-plugin-import": "^1.13.3",
        "babel-plugin-module-resolver": "^4.1.0",
        "commitizen": "^4.2.3",
        "cz-customizable": "^6.3.0",
        "eslint": "^7.14.0",
        "eslint-config-prettier": "^8.0.0",
        "eslint-import-resolver-typescript": "^2.4.0",
        "eslint-plugin-prettier": "^3.3.1",
        "eslint-plugin-react": "^7.20.5",
        "eslint-plugin-react-hooks": "^4.0.8",
        "husky": "^5.2.0",
        "inquirer": "^8.0.0",
        "jest": "^26.6.3",
        "jest-circus": "^26.6.3",
        "jest-fetch-mock": "^3.0.3",
        "lint-staged": "^10.5.4",
        "metro-react-native-babel-preset": "^0.64.0",
        "npm-run-all": "^4.1.5",
        "pont-engine": "1.0.13",
        "prettier": "^2.0.5",
        "react-native-iconfont-cli": "^2.2.1",
        "react-test-renderer": "^17.0.2",
        "shelljs": "^0.8.4",
        "ts-jest": "^26.5.1",
        "typescript": "^4.2.3"
    },
    "resolutions": {
        "@types/react": "^17"
    },
    "jest": {
        "preset": "react-native",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx",
            "json",
            "node"
        ]
    }
}